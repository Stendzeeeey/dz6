#include "pt4.h"
#include <fstream>
using namespace std;
void Solve ()
{
    Task("TheMassiv39");
    int x[10], n;
    fstream f_in;
    string s;
    pt >> s;
    f_in.open(s.c_str(), ios_base::in);
    f_in >> n;
    fstream f_out;
    string s_out;
    pt >> s_out;
    f-out.open(s.out.c_str(), ios_base::out);
    for (int i = 0; i < n; i++)
    {
        f_in >> x[i];
        if ((x[i]>=1) && (x[i]<=10))
        {
            f_out << '0' << ' ' << x[i] << ' ';
            }
            else { f_out << x[i] << ' ' ; }
        }
        f_out.close();
        f_in.close();
}
            
    
   
